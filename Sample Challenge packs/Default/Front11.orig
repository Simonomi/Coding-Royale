public class Front11 {{
	public static int[] front11(int[] a, int[] b) {
	if(a.length != 0 && b.length != 0) {
		int[] out = {a[0],b[0]};
		return out
	} else if(a.length > 0) {
		int[] out = {a[0]};
		return out;
	} else if(b.length > 0) {
		int[] out = {b[0]};
		return out;
	}
	int[] out = {};
	return out;
	}
	
	public static void main(String[] args) {
		int[] a = {1,2,3}
		int b = {4,5};
		int[] out = Front11(a, b);
		for(int i = 0; i < out.length; i++) {
			System.out.print(out[i] + " ");
		}
		System.out.println("");
	}
}
